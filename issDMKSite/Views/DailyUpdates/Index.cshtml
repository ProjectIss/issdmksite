@model IEnumerable<issDMKSite.Models.DailyUpdate>

@{
    ViewBag.Title = "Index";
}

<div class="card-header header-elements-inline" style="background-color: transparent;">
    <div class="list-icons" style=" float: right;">
      
            @Html.ActionLink("புதிதாக உருவாக்கு", "Create", null, new { @class = "btn btn-primary" })
        
    </div>
</div>
<table class="table">
    <tr>
        <th>
            Daily Detail
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.dailyDetail)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                @Html.ActionLink("Details", "Details", new { id = item.id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.id })
            </td>
        </tr>
    }

</table>
